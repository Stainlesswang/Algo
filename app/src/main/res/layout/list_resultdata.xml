<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="20dp"
        android:gravity="center">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <TextView
                android:id="@+id/up_grade"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="测试数据" />
        </LinearLayout>

        <View
            android:layout_width="1px"
            android:layout_height="fill_parent" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <TextView
                android:id="@+id/integral_mall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="选择物品编号" />
        </LinearLayout>

        <View
            android:layout_width="1px"
            android:layout_height="fill_parent" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="center">

            <TextView
                android:id="@+id/data_analysis"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="总价值" />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:gravity="center">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/testdata_ID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="模块一"/>
        </LinearLayout>
        <View
            android:layout_width="1px"
            android:layout_height="fill_parent" />
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/testdata_bianhao"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="模块二"/>
        </LinearLayout>
        <View
            android:layout_width="1px"
            android:layout_height="fill_parent" />
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/testdata_resultValue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="模块三"/>
        </LinearLayout>
    </LinearLayout>


    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="20dp"
        android:gravity="center">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/df"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="物品重量/背包容量"/>
        </LinearLayout>
        <View
            android:layout_width="1px"
            android:layout_height="fill_parent" />
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/dg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="耗时/ms"/>
        </LinearLayout>
        <View
            android:layout_width="1px"
            android:layout_height="fill_parent" />
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/re"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="匹配度"/>
        </LinearLayout>
        </LinearLayout>


    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="30dp"
        android:gravity="center">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/testdata_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="模块一"/>
        </LinearLayout>
        <View
            android:layout_width="1px"
            android:layout_height="fill_parent" />
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/testdata_weight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="模块二"/>
        </LinearLayout>
        <View
            android:layout_width="1px"
            android:layout_height="fill_parent" />
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/testdata_percent"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="模块三"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
